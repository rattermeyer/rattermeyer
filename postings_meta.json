---
layout: null
---
{
  "posts": {
    {% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "authors": [{% for author in post.authors %}"{{author}}"{% if forloop.last == false %},{% endif %} {% endfor %}],
      "date": "{{ post.date }}",
      "tags": [{% for tag in post.tags %}"{{tag}}"{% if forloop.last == false %},{% endif %} {% endfor %}]
    }
    {% endfor %}
  }
}
